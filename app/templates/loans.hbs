{{page-header title="Loans"}}
<div class="container">
  <div class="col-md-12">
    <div class="row">
      <div class="col-md-6">
        <h3>Your View</h3>
        {{loan-list/create-loan-form createLoan="createLoan"}}
        <div>
          <h4>Tracked Loans</h4>
        {{#each model as |loan|}}
        {{loan-list/card loan=loan deleteLoan="deleteLoan"}}
        {{/each}}
        </div>
      </div>
      <div class="col-md-6">
        <h3>Tools</h3>
        <h5>360 Loan Review</h5>
        <h5>Loan Visualization</h5>
      </div>
    </div>
  </div>
</div>
